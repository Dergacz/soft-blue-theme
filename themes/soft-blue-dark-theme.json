{
	"name": "Soft Blue Dark Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#242B3D",
		"editor.foreground": "#E8F4F8",
		
		"sideBar.background": "#232938",
		"sideBar.foreground": "#B8D4E3",
		"sideBar.border": "#2C3E50",
		"sideBarTitle.foreground": "#85C1E9",
		"sideBarSectionHeader.background": "#252B3D",
		"sideBarSectionHeader.foreground": "#AED6F1",
		
		"activityBar.background": "#1B2030",
		"activityBar.foreground": "#85C1E9",
		"activityBar.inactiveForeground": "#5D6D7E",
		"activityBarBadge.background": "#3498DB",
		"activityBarBadge.foreground": "#FFFFFF",
		"activityBar.border": "#2C3E50",
		
		"statusBar.background": "#2C3E50",
		"statusBar.foreground": "#AED6F1",
		"statusBar.noFolderBackground": "#34495E",
		"statusBar.debuggingBackground": "#2980B9",
		
		"tab.activeBackground": "#242B3D",
		"tab.activeForeground": "#E8F4F8",
		"tab.inactiveBackground": "#252B3D",
		"tab.inactiveForeground": "#85929E",
		"tab.border": "#2C3E50",
		"tab.activeBorder": "#3498DB",
		
		"titleBar.activeBackground": "#1E2332",
		"titleBar.activeForeground": "#AED6F1",
		"titleBar.inactiveBackground": "#16171A",
		"titleBar.inactiveForeground": "#5D6D7E",
		
		"quickInput.background": "#252B3D",
		"quickInput.foreground": "#E8F4F8",
		"pickerGroup.foreground": "#85C1E9",
		"pickerGroup.border": "#2C3E50",
		
		"terminal.background": "#242B3D",
		"terminal.foreground": "#E8F4F8",
		"terminal.ansiBlue": "#5DADE2",
		"terminal.ansiCyan": "#30D5C8",
		"terminal.ansiGreen": "#30D5C8",
		"terminal.ansiYellow": "#FFEDA1",
		"terminal.ansiRed": "#E74C3C",
		"terminal.ansiMagenta": "#17A2B8",
		
		"editor.selectionBackground": "#3498DB40",
		"editor.selectionHighlightBackground": "#2980B930",
		"editor.findMatchBackground": "#17A2B860",
		"editor.findMatchHighlightBackground": "#5DADE240",
		"editorCursor.foreground": "#5DADE2",
		"editor.lineHighlightBackground": "#232938",
		
		"editorLineNumber.foreground": "#5D6D7E",
		"editorLineNumber.activeForeground": "#85C1E9",
		"editorIndentGuide.background1": "#2C3E50",
		"editorIndentGuide.activeBackground1": "#3498DB",
		
		"button.background": "#3498DB",
		"button.foreground": "#FFFFFF",
		"button.hoverBackground": "#2980B9",
		"input.background": "#252B3D",
		"input.foreground": "#E8F4F8",
		"input.border": "#2C3E50",
		"dropdown.background": "#252B3D",
		"dropdown.foreground": "#E8F4F8",
		"dropdown.border": "#2C3E50",
		
		"notifications.background": "#252B3D",
		"notifications.foreground": "#E8F4F8",
		"notifications.border": "#2C3E50",
		
		"scrollbar.shadow": "#1A1F2E",
		"scrollbarSlider.background": "#3498DB40",
		"scrollbarSlider.hoverBackground": "#3498DB60",
		"scrollbarSlider.activeBackground": "#3498DB80",
		
		"debugToolBar.background": "#252B3D",
		"debugConsole.infoForeground": "#85C1E9",
		"debugConsole.warningForeground": "#E67E22",
		"debugConsole.errorForeground": "#E74C3C",
		
		"list.activeSelectionBackground": "#3498DB40",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.inactiveSelectionBackground": "#2C3E5030",
		"list.hoverBackground": "#2C3E5020",
		"list.focusBackground": "#3498DB30",
		
		"editorGroup.border": "#2C3E50",
		"editorGroupHeader.tabsBackground": "#1E2332",
		"editorGroupHeader.noTabsBackground": "#1A1F2E"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7B8794"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#B3D9FF"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#5DADE2"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#85C1E9"
			}
		},
		{
			"name": "Mathematical Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.comparison",
				"keyword.operator.logical",
				"punctuation.separator.operator",
				"keyword.operator.increment",
				"keyword.operator.decrement"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#E74C3C"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#30D5C8"
			}
		},
		{
			"name": "Function Parameters",
			"scope": [
				"variable.parameter",
				"meta.function.parameters variable.other"
			],
			"settings": {
				"foreground": "#85C1E9"
			}
		},
		{
			"name": "Language Variables (this, self, super)",
			"scope": [
				"variable.language",
				"variable.language.this",
				"variable.language.super",
				"variable.language.self"
			],
			"settings": {
				"foreground": "#FF8CCC",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Method Calls",
			"scope": [
				"entity.name.method",
				"meta.method-call"
			],
			"settings": {
				"foreground": "#66E5FF",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Object Properties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#B8D4E3"
			}
		},
		{
			"name": "Numbers and Constants",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#17A2B8"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#FFEDA1"
			}
		},
		{
			"name": "Classes and Types",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#85C1E9"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#FFEDA1"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFEDA1"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#E74C3C"
			}
		},
		{
			"name": "JSON Keys",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#85C1E9"
			}
		},
		{
			"name": "Markdown Headings",
			"scope": [
				"markdown.heading",
				"markup.heading | markup.heading entity.name",
				"markup.heading.markdown punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#85C1E9",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E8F4F8"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#D5DBDB"
			}
		},
		{
			"name": "Markdown Links",
			"scope": [
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#5DADE2",
				"fontStyle": "underline"
			}
		},
		{
			"name": "CSS Properties",
			"scope": [
				"support.type.property-name.css"
			],
			"settings": {
				"foreground": "#30D5C8"
			}
		},
		{
			"name": "CSS Values",
			"scope": [
				"support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#17A2B8"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.assignment",
				"keyword.operator.arithmetic",
				"keyword.operator.logical",
				"keyword.operator.comparison"
			],
			"settings": {
				"foreground": "#FF7085"
			}
		},
		{
			"name": "Control Flow",
			"scope": [
				"keyword.control.flow",
				"keyword.control.conditional",
				"keyword.control.loop"
			],
			"settings": {
				"foreground": "#FF8CCC"
			}
		},
		{
			"name": "Imports and Exports",
			"scope": [
				"keyword.control.import",
				"keyword.control.export",
				"keyword.control.from"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Enum Names",
			"scope": [
				"entity.name.type.enum",
				"support.type.enum"
			],
			"settings": {
				"foreground": "#30D5C8"
			}
		},
		{
			"name": "Enum Values",
			"scope": [
				"variable.other.enummember",
				"constant.other.enum"
			],
			"settings": {
				"foreground": "#FFEDA1"
			}
		},
		{
			"name": "Type Annotations",
			"scope": [
				"entity.name.type",
				"support.type",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#42FFC2"
			}
		}
	]
}
